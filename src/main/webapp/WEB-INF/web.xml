    <description></description>
    <servlet-name>authentification</servlet-name>
    <servlet-class>eu.ensup.gestionEleveV2.presentation.AuthentificationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>authentification</servlet-name>
    <url-pattern>/authentification</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>etudiantDetail</servlet-name>
    <servlet-class>eu.ensup.gestionEleveV2.presentation.EtudiantServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>etudiantDetail</servlet-name>
    <url-pattern>/etudiantDetail</url-pattern>
  </servlet-mapping>
</web-app>